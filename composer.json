{
    "name": "emedia/oxygen-push-notifications",
    "description": "Individual and group push notification library for Oxygen.",
    "type": "library",
    "repositories": [
        {
            "type":"vcs",
            "url":"https://github.com/keen2work/formation.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/quickdata-laravel.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/keen2work/laravel-helpers.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/keen2work/php-helpers.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/keen2work/devices-laravel.git"
        },
        {
            "type":"vcs",
            "url":"https://github.com/keen2work/lotus.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "elegantmedia/oxygen-foundation": "^3.0",
        "emedia/formation": "^2.0",
        "emedia/devices-laravel": "^5.0",
        "kreait/laravel-firebase": "^5.0"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "EMedia\\OxygenPushNotifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EMedia\\OxygenPushNotifications\\OxygenPushNotificationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
